{
    "name": "class-backend",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "tsc": "tsc",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "build:clean": "rimraf dist",
        "build": "npm run build:clean && tsc",
        "build:dev": "tsc -w",
        "start": "node dist/src/main.js",
        "start:build": "npm run build && npm start",
        "start:dev": "ts-node -r tsconfig-paths/register src/main.ts",
        "db:rollback": "npx knex migrate:rollback --knexfile knexfile.ts",
        "db:migrate:latest": "npx knex migrate:latest --knexfile knexfile.ts",
        "db:migrate:make": "npx knex migrate:make --knexfile knexfile.ts",
        "db:migrate:run": "npx knex migrate:latest --knexfile knexfile.ts",
        "db:seed:make": "npx knex seed:make --knexfile knexfile.ts",
        "db:seed:run": "npx knex seed:run --knexfile knexfile.ts"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.20.3",
        "dotenv": "^16.4.5",
        "dotenv-cli": "^7.4.2",
        "express": "^4.21.1",
        "express-validator": "^7.2.0",
        "knex": "^3.1.0",
        "pg": "^8.13.1",
        "rimraf": "^6.0.1",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@types/express": "^5.0.0",
        "@types/knex": "^0.15.2",
        "@types/node": "^22.9.0",
        "@typescript-eslint/eslint-plugin": "^8.13.0",
        "@typescript-eslint/parser": "^8.13.0",
        "eslint": "^9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "jest": "^29.7.0",
        "prettier": "^3.3.3",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.6.3"
    }
}
